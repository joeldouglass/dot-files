[user]
  name = Joel Douglass
  email = joel.douglass@velir.com
[alias]
  graph = log --graph --pretty=format:'%Cred%h%Creset - %Cgreen(%ad)%C(yellow)%d%Creset %s %C(bold blue)<%an>%Creset' --abbrev-commit --date=local
  co = checkout
  ci = commit
  st = status
  fe = fetch
  rb = rebase
[difftool]
  prompt = false
[core]
  visual = vim
  editor = vim
  autocrlf = false
  longpaths = true
[push]
  default = matching
[merge]
  tool = vimdiff
  conflictstyle = diff3
[mergetool]
  prompt = false
  keepBackup = false
[oh-my-zsh]
  hide-dirty = 1
